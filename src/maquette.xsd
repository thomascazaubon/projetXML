<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:complexType name ="Epreuve">
    <xs:attribute name="idEpreuve" type="xs:ID"/>
		<xs:attribute name="nomEpreuve" type="xs:string"/>
