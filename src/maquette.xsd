  <xs:keyref name="idGrandDomaineKeyRef" refer="idGrandDomaineKey">
    <xs:selector xpath="expression_XPath" />
    <xs:field xpath="expression_XPath" />
  </xs:keyref>

  <xs:keyref name="idSemestreKeyRef" refer="idSemestreKey">
